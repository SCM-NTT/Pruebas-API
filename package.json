{
  "name": "pruebas-api",
  "version": "1.0.0",
  "description": "Este proyecto se va a centrar en desarrollar una API y en su integraci√≥n en un proyecto web",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "concurrently \"npm run update_global\" \"npm run update_ui\" \"npm run update_api\"",
    "update_global":"npm install",
    "update_ui": "cd ./frontend && npm install",
    "update_api": "cd ./backend && npm install",
    "dev": "concurrently \"npm run dev_ui\" \"npm run dev_api\"",
    "dev_ui": "cd ./frontend && npm run dev",
    "dev_api": "cd ./backend && npm run dev"
  },
